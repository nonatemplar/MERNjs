{
  "name": "MERNjs",
  "version": "1.0.0",
  "description": "A Open-Source Full-Stack Solution For MERN (MongoDB, ExpressJS, ReactJS and NodeJS) Applications.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:IamMohaiminul/MERNjs.git"
  },
  "author": "Muhammad Mohaiminul Islam <IamMohaiminul>",
  "license": "MIT",
  "engines": {
    "node": "~6.x"
  },
  "scripts": {
    "start": "node ./build",
    "debug": "node --debug ./build",
    "build": "npm run clean:server && npm run build:server && npm run build:client",
    "clean:server": "rm -rf build && mkdir build",
    "build:server": "babel -d ./build ./server -s",
    "build:client": "webpack --progress --profile",
    "live:server": "nodemon --exec babel-node ./server",
    "live:server:debug": "DEBUG=MERNjs:* npm run live:server",
    "live:client": "webpack --watch --progress --profile"
  },
  "dependencies": {
    "app-module-path": "~2.2.0",
    "axios": "~0.15.3",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "config": "~1.25.1",
    "cookie-parser": "~1.4.3",
    "cors": "~2.8.1",
    "debug": "~2.6.3",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "lodash": "~4.17.4",
    "moment": "^2.22.2",
    "mongoose": "^5.2.4",
    "morgan": "^1.9.0",
    "pug": "~2.0.0-beta11",
    "serve-favicon": "~2.4.1"
  },
  "devDependencies": {
    "babel-cli": "~6.24.0",
    "babel-core": "~6.24.0",
    "babel-loader": "~6.4.1",
    "babel-preset-env": "~1.2.2",
    "babel-preset-react": "~6.23.0",
    "bootstrap": "~3.3.7",
    "clean-webpack-plugin": "~0.1.16",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "~0.27.3",
    "extract-text-webpack-plugin": "~2.1.0",
    "file-loader": "~0.10.1",
    "font-awesome": "~4.7.0",
    "jquery": "^3.3.1",
    "node-sass": "^4.9.2",
    "nodemon": "~1.11.0",
    "react": "~15.4.2",
    "react-dom": "~15.4.2",
    "react-redux": "~5.0.3",
    "react-router": "~3.0.2",
    "redux": "~3.6.0",
    "redux-logger": "~2.10.2",
    "redux-thunk": "~2.2.0",
    "sass-loader": "~6.0.3",
    "style-loader": "~0.15.0",
    "toastr": "~2.1.2",
    "url-loader": "^1.0.1",
    "webpack": "^2.3.3"
  },
  "homepage": "http://mern.js.org/"
}
